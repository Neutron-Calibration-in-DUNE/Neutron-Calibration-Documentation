@article{Weitzel,
title = {ZEKE-PEPICO investigations of dissociation energies in ionic reactions},
journal = {Chemical Physics Letters},
volume = {224},
number = {3},
pages = {371-380},
year = {1994},
issn = {0009-2614},
doi = {https://doi.org/10.1016/0009-2614(94)00567-2},
url = {https://www.sciencedirect.com/science/article/pii/0009261494005672},
author = {Karl-Michael Weitzel and Joachim Mähnert and Martin Penno},
abstract = {A coincidence experiment between zero kinetic energy photoelectrons (ZEKE-PE) and the corresponding photoions (PI) has been set up. In this experiment electrons and ions are extracted from an ion source in a delayed pulsed field. The high electron energy resolution is based on time of flight discrimination of the electrons. This ZEKE-PEPICO technique has been applied to the threshold of the reactios: (i) C2H2+ → C2H+ + H and (ii) H2+ → H+ + H. The example of H loss from acetylene ions illustrates the power of the technique, while the dissociative ionization of H2+ represent a critical test for the ZEKE-PEPICO spectrometer.}
}
